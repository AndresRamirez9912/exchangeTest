{
  "name": "apex-web-implementation-template",
  "version": "1.0.45",
  "dependencies": {
    "alphapoint": "3.2.16",
    "apex-chart-react": "0.4.3",
    "apex-web": "^0.7.192",
    "bignumber.js": "7.0.2",
    "classnames": "^2.2.5",
    "coinslot": "^1.1.0",
    "cross-fetch": "^2.1.0",
    "crypto-js": "^3.1.9-1",
    "css-vars-ponyfill": "^1.15.3",
    "date-fns": "^1.29.0",
    "detect-emoji-support": "^0.1.5",
    "husky": "^0.14.3",
    "jstimezonedetect": "^1.0.6",
    "libphonenumber-js": "^1.5.1",
    "lint-staged": "^6.1.1",
    "numeral": "^2.0.6",
    "parse-decimal-number": "^1.0.0",
    "qrcode.react": "^0.8.0",
    "react": "^16.11.0",
    "react-bem-helper": "^1.4.1",
    "react-dom": "^16.11.0",
    "react-inlinesvg": "^0.8.1",
    "react-input-mask": "^2.0.4",
    "react-loadable": "^5.4.0",
    "react-modal": "^3.6.1",
    "react-phone-number-input": "^2.2.14",
    "react-redux": "^5.0.7",
    "react-redux-snackbar": "^1.1.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "1.1.5",
    "react-sliding-pane": "^2.1.1",
    "react-table": "^6.8.6",
    "redux": "^3.7.2",
    "redux-form": "^7.3.0",
    "redux-i18n": "^1.5.14",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json}": [
      "prettier --write --single-quote --jsx-bracket-same-line",
      "git add"
    ]
  },
  "scripts": {
    "precommit": "lint-staged",
    "start": "react-scripts start",
    "react-scripts-build": "react-scripts build && npm run write-version-file",
    "build": "node ./scripts/build.js",
    "build-static-ci": "node ./scripts/build.js&&npm run build-rename&&cp -R build/static ../npm",
    "build-static": "GENERATE_SOURCEMAP=false PUBLIC_URL=/ node ./scripts/build.js&&npm run build-rename&&cp -R build/static ../npm",
    "build-rename": "npm run build-rename-js&&npm run build-rename-css&&npm run build-fix-html&&npm run build-fix-web-config&&rehype build/index.html --output build/index.html --use rehype-format",
    "build-rename-js": "renamer --find \"/main\\.[^\\.]+\\./\" --replace \"main.\" build/static/js/*",
    "build-rename-css": "renamer --find \"/main\\.[^\\.]+\\./\" --replace \"main.\" build/static/css/*",
    "build-fix-html": "replace \"main(\\.[^\\.]+)\" \"main\" --path-element name build/index.html",
    "build-fix-web-config": "replace \"/web/\" \"/\" --path-element name build/Web.config",
    "lib": "rm -rf lib && cross-env NODE_ENV=production babel src --out-dir lib --copy-files --ignore __tests__,spec.js,test.js,__snapshots__",
    "test": "react-scripts test --env=jsdom",
    "coverage": "npm test -- --coverage",
    "eject": "react-scripts eject",
    "extract": "i18n_extract",
    "import": "i18n_import --translations=src/translations",
    "analyze": "source-map-explorer build/static/js/main.*",
    "prepublishOnly": "npm run lib",
    "write-version-file": "node scripts/versionFileGenerator.js",
    "upgrade-awt": "node scripts/upgrade-awt.js"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "cross-env": "^5.2.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "fs-extra": "^7.0.0",
    "ftp-deploy": "^2.1.3",
    "prettier": "1.13.7",
    "redux-mock-store": "^1.5.1",
    "rehype-cli": "^6.0.0",
    "rehype-format": "^2.3.0",
    "renamer": "^1.0.0",
    "replace": "^1.0.0",
    "shelljs": "^0.8.2",
    "source-map-explorer": "^1.5.0"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "registry": "http://nexus.alphapoint.com:8087/repository/npm-private/"
  }
}
