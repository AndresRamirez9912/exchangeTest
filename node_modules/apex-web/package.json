  "lint-staged": {
    "src/**/*.{js,jsx,json}": [
      "prettier --write --single-quote --jsx-bracket-same-line",
      "git add"
    ]
  },
  "scripts": {
    "precommit": "lint-staged",
    "start": "react-scripts start",
    "react-scripts-build": "react-scripts build && npm run write-version-file",
    "build": "CI=false node ./scripts/build.js",
    "build-static-ci": "node ./scripts/build.js&&npm run build-rename&&cp -R build/static ../npm",
    "build-static": "GENERATE_SOURCEMAP=false PUBLIC_URL=/ node ./scripts/build.js&&npm run build-rename&&cp -R build/static ../npm",
    "build-rename": "npm run build-rename-js&&npm run build-rename-css&&npm run build-fix-html&&npm run build-fix-web-config&&rehype build/index.html --output build/index.html --use rehype-format",
    "build-rename-js": "renamer --find \"/main\\.[^\\.]+\\./\" --replace \"main.\" build/static/js/*",
    "build-rename-css": "renamer --find \"/main\\.[^\\.]+\\./\" --replace \"main.\" build/static/css/*",
    "build-fix-html": "replace \"main(\\.[^\\.]+)\" \"main\" --path-element name build/index.html",
    "build-fix-web-config": "replace \"/web/\" \"/\" --path-element name build/Web.config",
    "prelib": "npm run extract",
    "lib": "rm -rf lib && npm install && cross-env NODE_ENV=production babel src --out-dir lib --copy-files --ignore __tests__,spec.js,test.js,__snapshots__",
    "postlib": "cp -r locales/ lib/locales",
    "test": "react-scripts test --env=jsdom",
    "coverage": "npm test -- --coverage",
    "eject": "react-scripts eject",
    "extract": "i18n_extract",
    "import": "i18n_import --translations=src/translations",
    "analyze": "source-map-explorer build/static/js/main.*",
    "prepublishOnly": "npm run lib",
    "write-version-file": "node scripts/versionFileGenerator.js",
    "upgrade-awt": "node scripts/upgrade-awt.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.8",
    "@babel/compat-data": "7.19.4",
    "@babel/core": "7.19.6",
    "@babel/generator": "7.19.6",
    "@babel/helper-compilation-targets": "7.19.3",
    "@babel/helper-create-class-features-plugin": "7.19.0",
    "@babel/helper-module-transforms": "7.19.6",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.8.3",
    "@babel/runtime": "^7.16.7",
    "cross-env": "^5.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "fs-extra": "^7.0.0",
    "ftp-deploy": "^2.1.3",
    "prettier": "1.13.7",
    "redux-mock-store": "^1.5.1",
    "rehype-cli": "^6.0.0",
    "rehype-format": "^2.3.0",
    "renamer": "^1.0.0",
    "replace": "^1.0.0",
    "shelljs": "^0.8.2",
    "source-map-explorer": "^1.5.0"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "registry": "http://nexus.alphapoint.com:8087/repository/npm-private/"
  },
  "browserslist": [
    ">0.2%",
    "not ie <= 11",
    "not op_mini all"
  ]
}
