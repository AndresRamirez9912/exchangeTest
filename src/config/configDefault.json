                "validators": ["required"]
              },
              {
                "label": "Billing Street Address",
                "name": "billingStreetAddress",
                "type": "text",
                "info": "(100 Washington Way)",
                "validators": ["required"]
              },
              {
                "label": "Billing State",
                "name": "state",
                "type": "text",
                "info": "Abbreviation only",
                "validators": ["required"]
              },
              {
                "label": "Billing City",
                "name": "billingCity",
                "type": "text",
                "validators": ["required"]
              },
              {
                "label": "Billing Zip",
                "name": "billingZip",
                "type": "text",
                "validators": ["required", "alphanumeric"]
              }
            ]
          }
        ]
      },
      {
        "level": 2,
